{
  "name": "api-node-ts",
  "version": "1.0.0",
  "description": "Typescript node.js API",
  "main": "index.js",
  "scripts": {
    "build": "tsc",
    "start": "yarn build && node dist/src/index.js",
    "start:local": "ts-node-dev src/index.ts",
    "lint": "eslint ./src ./test --ext .ts",
    "lint:fix": "eslint ./src ./test --ext .ts --fix",
    "test:functional": "jest --projects ./test --forceExit --runInBand",
    "test": "echo \"Error: no test specified\" && exit 1",
    "style:check": "prettier --check \"src/**/*.ts\" \"test/**/*.ts\"",
    "style:fix": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\""
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/ikytussistemas/API-NODE-TS.git"
  },
  "author": "Franz√© Silva",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/ikytussistemas/API-NODE-TS/issues"
  },
  "homepage": "https://github.com/ikytussistemas/API-NODE-TS#readme",
  "devDependencies": {
    "@types/express": "^4.17.9",
    "@types/jest": "^26.0.15",
    "@types/module-alias": "^2.0.0",
    "@types/node": "^14.14.9",
    "@types/supertest": "^2.0.10",
    "@typescript-eslint/eslint-plugin": "^4.8.1",
    "@typescript-eslint/parser": "^4.8.1",
    "eslint": "^7.13.0",
    "jest": "^26.6.3",
    "prettier": "^2.2.0",
    "supertest": "^6.0.1",
    "ts-jest": "^26.4.4",
    "ts-node-dev": "^1.0.0",
    "typescript": "^4.1.2"
  },
  "dependencies": {
    "@overnightjs/core": "^1.7.5",
    "body-parser": "^1.19.0",
    "express": "^4.17.1",
    "module-alias": "^2.2.2"
  }
}
